<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xiyan.testapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.supconit.azpt.lib.AZPTApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".IocActivity"
            android:label="@string/title_activity_ioc"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DBActivity"
            android:label="@string/title_activity_db"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NetworkActivity"
            android:label="@string/title_activity_network"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TestActivity"
            android:label="@string/title_activity_test"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChartActivity"
            android:label="@string/title_activity_chart"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LogActivity"
            android:label="@string/title_activity_log"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
